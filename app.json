{
  "expo": {
    "name": "TiltSync",
    "slug": "tiltsync",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a5490"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.altmake.TiltSync",
      "buildNumber": "11",
      "appleTeamId": "S27QQ5M9D5",
      "infoPlist": {
        "CFBundleDisplayName": "TiltSync",
        "LSApplicationCategoryType": "public.app-category.utilities",
        "NSLocationWhenInUseUsageDescription": "TiltSync needs your location to calculate the optimal panel angle for your latitude.",
        "NSMotionUsageDescription": "TiltSync uses motion sensors to measure your panel's current tilt angle.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a5490"
      },
      "package": "com.altmake.tiltsync",
      "versionCode": 11,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withIphoneOnly.js",
      [
        "expo-build-properties",
        {
          "ios": {
            "extraPodfileProperties": {
              "IPHONEOS_DEPLOYMENT_TARGET": "15.1"
            }
          }
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "TiltSync needs your location to calculate the optimal panel angle for your latitude."
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "TiltSync uses motion sensors to measure your panel's current tilt angle."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b01a1e77-a592-4f48-ae69-01e25524067e"
      }
    }
  }
}
